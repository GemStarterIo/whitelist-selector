{
  "name": "@synapsenetwork/synapsenetwork-whitelist",
  "version": "0.0.1",
  "description": "Synapse Network Whitelist Contract",
  "license": "MIT",
  "scripts": {
    "clean": "hardhat clean",
    "compile": "hardhat --show-stack-traces --max-memory 8192 compile",
    "coverage": "HIDE_DEPLOY_LOG=true OPTIMIZER_DISABLED=true hardhat coverage --solcoverjs ./.solcover.js --temp artifacts --testfiles \"./test/**/*.ts\"",
    "deploy": "hardhat deploy --write true --network",
    "verify": "hardhat etherscan-verify --license MIT --solc-input --network",
    "etherscan-verify": "hardhat etherscan:verify --network",
    "lint": "yarn run lint:sol && yarn run lint:ts && yarn run prettier:list-different",
    "lint:sol": "solhint --config ./.solhint.json --max-warnings 0 \"contracts/**/*.sol\"",
    "lint:ts": "eslint --config ./.eslintrc.yaml --ignore-path ./.eslintignore --ext .js,.ts .",
    "prettier": "prettier --config .prettierrc --write \"**/*.{js,json,md,sol,ts}\"",
    "prettier:list-different": "prettier --config .prettierrc --list-different \"**/*.{js,json,md,sol,ts}\"",
    "test": "HIDE_DEPLOY_LOG=true OPTIMIZER_DISABLED=true hardhat test",
    "typechain": "hardhat typechain"
  },
  "dependencies": {
    "@ethersproject/abstract-signer": "^5.3.0",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.3",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^4.1.0",
    "@chainlink/contracts": "^0.1.7",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.0.2",
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.12.2",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "chai": "^4.3.4",
    "chai-ethers": "^0.0.1",
    "chalk": "^4.1.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eth-sig-util": "^3.0.1",
    "ethereum-waffle": "^3.3.0",
    "ethereumjs-util": "^7.0.10",
    "ethers": "^5.3.1",
    "fs-extra": "^10.0.0",
    "hardhat": "^2.4.0",
    "hardhat-abi-exporter": "^2.2.1",
    "hardhat-contract-sizer": "^2.0.3",
    "hardhat-deploy": "^0.8.6",
    "hardhat-deploy-ethers": "^0.3.0-beta.10",
    "hardhat-gas-reporter": "^1.0.4",
    "hardhat-watcher": "^2.1.1",
    "mocha": "^9.0.0",
    "prettier": "^2.3.1",
    "prettier-plugin-solidity": "1.0.0-beta.13",
    "shelljs": "^0.8.4",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.16",
    "ts-node": "^10.0.0",
    "typechain": "^5.0.0",
    "typescript": "^4.3.3"
  },
  "files": [
    "build/**",
    "artifacts/**",
    "contracts/**",
    "deployments/**",
    "deploy/**"
  ]
}
